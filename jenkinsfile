pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                   git branch: 'B2C', url: 'https://github.com/zgolliaya/PFE2.git'
            }
        }
        
        
        stage('Run Tests') {
            steps {
              
               sh "pip install robotframework-seleniumlibrary"
               sh "  apt-get update -y "
               sh "  apt-get install -y chromium-driver "
               

               sh  " robot ./test2/add_user_management.robot"

                
            }
        }
    }


}
